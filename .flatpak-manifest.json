   "desktop-file-name-suffix": " (Nightly)",
   "finish-args": [
      "--share=ipc",
      "--socket=fallback-x11",
      "--socket=wayland",
      "--filesystem=xdg-documents"
   ],
   "id": "org.kde.kglife",
